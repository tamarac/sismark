[class^="ico-"], [class*="icon-"] {
	background-image: url("../img/sprite.png");
	background-repeat: no-repeat;
	display: inline-block;
	line-height:20px;
	margin: 1px;
	vertical-align: text-top;
}
{{#items}}
.ico-{{name}} {
	width: {{px.width}};
	height: {{px.height}};
	background-position: {{px.offset_x}} {{px.offset_y}};
}
{{/items}}